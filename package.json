{
  "name": "financial-organizer-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:financial-organizer-front": "node dist/financial-organizer-front/server/server.mjs",
